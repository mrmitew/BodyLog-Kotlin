<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.github.mrmitew.bodylog">

    <application
        android:name=".framework.AndroidApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.github.mrmitew.bodylog.framework.profile.details.view.ProfileDetailsActivity"/>
        <activity android:name="com.github.mrmitew.bodylog.framework.profile.edit.view.ProfileEditActivity">
        </activity>
        <activity android:name=".framework.dashboard.view.DashboardActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
    </application>

</manifest>